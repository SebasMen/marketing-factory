{
  "name": "templates",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/templates/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "webpack-cli build",
        "args": ["node-env=production"]
      },
      "configurations": {
        "development": {
          "args": ["node-env=development"]
        }
      }
    },
    "build:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p apps/templates/tsconfig.app.json --sourceMap && tsc-alias -p apps/templates/tsconfig.app.json",
        "cwd": "."
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nodemon --config apps/templates/nodemon.json",
        "cwd": "."
      }
    },
    "test": {
      "options": {
        "passWithNoTests": true
      }
    }
  }
}
